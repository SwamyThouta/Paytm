import React, { ChangeEvent } from "react";

interface Props {
  placeholder: string;
  id: string;
  name?: string;
  inputValue?: string;
  focus?: boolean;
  onChange?: (e: ChangeEvent<HTMLInputElement>) => void;
  autocomplete: boolean;
  // options?: { [key: string]: string | number }[];
}

export default function Search({ placeholder, id, name, onChange }: Props) {
  return (
    <>
      <div>
        <input
          type="search"
          name={name}
          id={id}
          className="bg-[#abb6b8] rounded-full py-1 px-5 placeholder:text-[#363636] placeholder:text-xs w-full
          outline-none"
          onChange={onChange}
          placeholder={placeholder}
          autoComplete="off"
        />
      </div>
    </>
  );
}
